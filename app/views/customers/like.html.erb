<table>
  <caption>いいね一覧</caption>
  <thead>
    <tr>
      <th>ジャンル</th>
      <th>タイトル</th>
      <th>レビュー</th>
    </tr>
  </thead>
  <tbody>
    <% @likes.each do |like| %>
      <tr>
        <td><%= like.review.book.genre.name %></td>
        <td><%= like.review.book.title %></td>
        <td><%= like.review.review %></td>
      </tr>
    <% end %>
  </tbody>
</table>